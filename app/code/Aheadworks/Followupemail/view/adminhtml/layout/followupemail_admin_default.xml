<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Aheadworks_Followupemail::css/style.css"/>
    </head>
    <body>
        <referenceContainer name="page.main.actions">
            <block class="Aheadworks\Followupemail\Block\Adminhtml\Menu" name="aheadworks_followupemail_menu" />
        </referenceContainer>
        <referenceContainer name="page.content">
            <container name="followupemail_breadcrumbs" before="messages.wrapper" as="followupemail_breadcrumbs" htmlTag="div" htmlClass="followupemail-breadcrumbs">
                <block class="Aheadworks\Followupemail\Block\Adminhtml\Breadcrumbs" name="breadcrumbs" as="breadcrumbs"/>
            </container>
        </referenceContainer>
    </body>
</page>
