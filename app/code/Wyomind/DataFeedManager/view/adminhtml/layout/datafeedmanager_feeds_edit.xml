<?xml version="1.0"?><!--  Copyright Â© 2015 Wyomind. All rights reserved.  See LICENSE.txt for license details. --><!-- Layout of the edit form of a google shopping data feed --><page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">    <update handle="editor"/>    <!-- add css and js files -->    <head>        <css src="Wyomind_DataFeedManager::css/datafeedmanager.css"/>        <css src="Wyomind_Core::js/codemirror/codemirror.css"/>        <link src="Wyomind_Core::js/codemirror/codemirror.js"/>        <link src="Wyomind_Core::js/codemirror/formatting.js"/>        <link src="Wyomind_Core::js/codemirror/active-line.js"/>        <link src="Wyomind_Core::js/codemirror/matchbrackets.js"/>        <link src="Wyomind_Core::js/codemirror/htmlmixed/htmlmixed.js"/>        <link src="Wyomind_Core::js/codemirror/xml/xml.js"/>        <link src="Wyomind_Core::js/codemirror/javascript/javascript.js"/>        <link src="Wyomind_Core::js/codemirror/css/css.js"/>        <link src="Wyomind_Core::js/codemirror/clike/clike.js"/>        <link src="Wyomind_Core::js/codemirror/php/php.js"/>        <link src="Wyomind_DataFeedManager::js/feeds/edit.js"/>    </head>        <!-- translation -->    <referenceContainer name="page.js.translate">        <block class="Magento\Framework\View\Element\Template" name="js_translate" as="js_translate" template="Wyomind_DataFeedManager::edit/translate.phtml"/>    </referenceContainer>        <!-- main block -->    <referenceContainer name="content">        <block class="Wyomind\DataFeedManager\Block\Adminhtml\Feeds\Edit" name="datafeedmanager_feeds_edit"/>    </referenceContainer>        <!-- left tabs -->    <referenceContainer name="left">        <block class="Wyomind\DataFeedManager\Block\Adminhtml\Feeds\Edit\Tabs" name="datafeedmanager_feeds_edit_tabs">            <!-- decalre blocks to use in the addTab method -->            <block class="Wyomind\DataFeedManager\Block\Adminhtml\Feeds\Edit\Tab\Configuration" name="datafeedmanager_feeds_edit_tab_configuration"/>            <block class="Wyomind\DataFeedManager\Block\Adminhtml\Feeds\Edit\Tab\Categories" name="datafeedmanager_feeds_edit_tab_categories"/>            <block class="Wyomind\DataFeedManager\Block\Adminhtml\Feeds\Edit\Tab\Filters" name="datafeedmanager_feeds_edit_tab_filters"/>            <block class="Wyomind\DataFeedManager\Block\Adminhtml\Feeds\Edit\Tab\Cron" name="datafeedmanager_feeds_edit_tab_cron"/>            <block class="Wyomind\DataFeedManager\Block\Adminhtml\Feeds\Edit\Tab\Ftp" name="datafeedmanager_feeds_edit_tab_ftp"/>            <!-- add tabs -->            <action method="addTab">                <argument name="name" xsi:type="string">configuration_section</argument>                <argument name="block" xsi:type="string">datafeedmanager_feeds_edit_tab_configuration</argument>            </action>            <action method="addTab">                <argument name="name" xsi:type="string">categories_section</argument>                <argument name="block" xsi:type="string">datafeedmanager_feeds_edit_tab_categories</argument>            </action>            <action method="addTab">                <argument name="name" xsi:type="string">filters_section</argument>                <argument name="block" xsi:type="string">datafeedmanager_feeds_edit_tab_filters</argument>            </action>            <action method="addTab">                <argument name="name" xsi:type="string">cron_section</argument>                <argument name="block" xsi:type="string">datafeedmanager_feeds_edit_tab_cron</argument>            </action>            <action method="addTab">                <argument name="name" xsi:type="string">ftp_section</argument>                <argument name="block" xsi:type="string">datafeedmanager_feeds_edit_tab_ftp</argument>            </action>        </block>    </referenceContainer></page>