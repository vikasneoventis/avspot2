        <referenceContainer name="content">
            <block class="Wyomind\DataFeedManager\Block\Adminhtml\Feeds" name="datafeedmanager_feeds"/>
            <block template="Wyomind_DataFeedManager::importDataFeed.phtml" class="Wyomind\DataFeedManager\Block\Adminhtml\ImportDataFeed" name="wyomind_datafeedmanager_import"/>
        </referenceContainer>
    </body>
</page>