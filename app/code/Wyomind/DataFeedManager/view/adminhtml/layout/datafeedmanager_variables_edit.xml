<?xml version="1.0"?><!--  Copyright Â© 2015 Wyomind. All rights reserved.  See LICENSE.txt for license details. --><!-- Layout of the edit form of a google shopping data feed --><page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">    <update handle="editor"/>    <!-- add css and js files -->    <head>        <css src="Wyomind_DataFeedManager::css/datafeedmanager.css"/>        <css src="Wyomind_Core::js/codemirror/codemirror.css"/>        <link src="Wyomind_Core::js/codemirror/codemirror.js"/>        <link src="Wyomind_Core::js/codemirror/formatting.js"/>        <link src="Wyomind_Core::js/codemirror/active-line.js"/>        <link src="Wyomind_Core::js/codemirror/matchbrackets.js"/>        <link src="Wyomind_Core::js/codemirror/htmlmixed/htmlmixed.js"/>        <link src="Wyomind_Core::js/codemirror/xml/xml.js"/>        <link src="Wyomind_Core::js/codemirror/javascript/javascript.js"/>        <link src="Wyomind_Core::js/codemirror/css/css.js"/>        <link src="Wyomind_Core::js/codemirror/clike/clike.js"/>        <link src="Wyomind_Core::js/codemirror/php/php.js"/>        <link src="Wyomind_DataFeedManager::js/variables/edit.js"/>    </head>        <!-- translation -->    <referenceContainer name="page.js.translate">        <block class="Magento\Framework\View\Element\Template" name="js_translate" as="js_translate" template="Wyomind_DataFeedManager::edit/translate.phtml"/>    </referenceContainer>        <!-- main block -->    <referenceContainer name="content">        <block class="Wyomind\DataFeedManager\Block\Adminhtml\Variables\Edit" name="datafeedmanager_variables_edit"/>    </referenceContainer>    </page>